<%= link_to "Log out", destroy_app_user_session_path, method: :delete %>
<h1>Welcome, <%= current_user.first_name %></h1>
<% if current_user.driver? %>
  <%= render 'driver' %>
<% else %>
  <%= render 'international' %>
<% end %>

<%= button_to "Your profile", app_user_path(current_user), method: :get, class: "advance-btn" %>
<% if current_user.admin? %>
  <%= button_to "Admin dashboard", app_admin_dashboard_path, method: :get, class: "advance-btn" %>
<% end %>